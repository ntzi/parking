{
	"name": "parking",
	"version": "0.0.1",
	"description": "",
	"type": "module",
	"engines": {
		"node": ">= 18.16.0"
	},
	"license": "Apache-2.0",
	"scripts": {
		"start": "NODE_ENV=production node build/src/app.js",
		"dev": "NODE_ENV=local ./scripts/startLocal.sh",
		"test": "npm run doc-test -- npx c8 ava --serial",
		"test:watch": "NODE_ENV=test ava --serial --watch",
		"test-local": "NODE_ENV=testLocal c8 ava --serial",
		"test-local:watch": "NODE_ENV=testLocal ava --serial --watch",
		"clean": "rimraf coverage build tmp",
		"build": "tsc",
		"build:watch": "tsc -w -p tsconfig.json",
		"build:release": "npm run clean && tsc -p tsconfig.release.json",
		"doc": "docker exec -it parking-api",
		"doc-test": "docker exec -i parking-api-test",
		"typeorm": "typeorm-ts-node-esm -d src/api/loaders/appDataSource.ts",
		"db:migration-generate": "npm run doc -- npm run typeorm -- migration:generate src/api/migrations/latest",
		"db:migration-run": "npm run doc -- npm run typeorm -- migration:run",
		"db:migration-revert": "npm run doc -- npm run typeorm -- migration:revert",
		"format": "prettier --write .",
		"lint": "eslint . --max-warnings 0 --report-unused-disable-directives",
		"lint:fix": "npm run lint -- --fix"
	},
	"dependencies": {
		"amqplib": "^0.10.3",
		"axios": "^1.3.6",
		"babel-jest": "^29.5.0",
		"cors": "^2.8.5",
		"csv-parse": "^5.3.8",
		"dayjs": "^1.11.7",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-validator": "^7.0.1",
		"joi": "^17.9.2",
		"jsonwebtoken": "^9.0.0",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"nodemon": "^2.0.22",
		"pg": "^8.10.0",
		"reflect-metadata": "^0.1.13",
		"supertest": "^6.3.3",
		"typeorm": "^0.3.16"
	},
	"devDependencies": {
		"@ava/typescript": "^4.0.0",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/express": "^4.17.17",
		"@types/node": "^18.16.0",
		"@typescript-eslint/eslint-plugin": "~5.59",
		"@typescript-eslint/parser": "~5.59",
		"ava": "^5.2.0",
		"c8": "^7.13.0",
		"eslint": "~8.39",
		"eslint-config-prettier": "~8.8",
		"eslint-plugin-jest": "~27.2",
		"prettier": "~2.8",
		"rimraf": "~5.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"tsutils": "~3.21",
		"typescript": "^5.0.4"
	}
}
